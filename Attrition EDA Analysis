    "### Age Vs Attrition"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 40,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 1152x576 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "myData[\"Age_Group\"] = \"\"\n",
    "myData.loc[myData[\"Age\"].between(18,24),\"Age_Group\"] = \"18-24\"\n",
    "myData.loc[myData[\"Age\"].between(25,31),\"Age_Group\"] = \"25-31\"\n",
    "myData.loc[myData[\"Age\"].between(32,38),\"Age_Group\"] = \"32-38\"\n",
    "myData.loc[myData[\"Age\"].between(39,45),\"Age_Group\"] = \"39-45\"\n",
    "myData.loc[myData[\"Age\"].between(46,52),\"Age_Group\"] = \"46-52\"\n",
    "myData.loc[myData[\"Age\"] >= 53,\"Age_Group\"] = \">=53\"\n",
    "\n",
    "\n",
    "ageGroupsVSattrition = pd.crosstab(index=myData[\"Age_Group\"],\n",
    "                                   columns=myData[\"Attrition\"],\n",
    "                                   normalize= \"index\"\n",
    "                                  )\n",
    "\n",
    "ageGroupsVSattrition.sort_values('Age_Group',inplace=True, ascending = False)\n",
    "ageGroupsVSattrition.plot(kind=\"barh\",stacked=True,figsize = (16,8), legend = False, xlabel = 'Age Group', ylabel = '% Attrition')\n",
    "plt.legend(loc = 'upper right')\n",
    "plt.title('Age Group VS Attrition')\n",
    "plt.xticks(rotation= 0)\n",
    "plt.savefig(\"AgeGroupVSAttritionH.png\",dpi=300)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### Inferences"
   ]
  },
